# look at light-evoked response to get a measure of electrotonic decay along dendrites to soma with voltage clamp
#
# scontrast 0.012, minten -0.05
#  irad1 0 orad1 50, 50-75, 75-100, 100-
#   g_dbp1_sbac 0.4e-10
#   sbac_vpulse -0.07
#   morph_frac 1
#
rdsgc_sbac_r --sbac_r 0 --g_dbp1_sbac 0.4e-10 --camid 0.0e-3 --catmid 5e-3 --catdist 4.0e-3 --g_sbac_sbac 2e-10 --sbac_synspac 16 --sbac_maxsdist 5 --g_sbac_dsgc 10e-10 --sbarr 0 --dsgc_x -200 --dsgc_y 50 --dbpthr -0.052 --mglur2 1 --n_am 0 --n_am2 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --sdia 0.3 --spdia 0.6 --n_dsgc 1 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.1 --n_dsgc 0 --stimtype 6 --irad1 0 --orad1 50 --sbac_vhold -0.07 --stimtime 0.5 --scontrast 0.012 --minten -0.05 --run_vclamp_sbac 1 --morph_frac 1 --sbac_vpulse -0.07 --sb_denddia 2.2 --predur 0.2 --filename x913

rdsgc_sbac_r --sbac_r 0 --g_dbp1_sbac 0.4e-10 --camid 0.0e-3 --catmid 5e-3 --catdist 4.0e-3 --g_sbac_sbac 2e-10 --sbac_synspac 16 --sbac_maxsdist 5 --g_sbac_dsgc 10e-10 --sbarr 0 --dsgc_x -200 --dsgc_y 50 --dbpthr -0.052 --mglur2 1 --n_am 0 --n_am2 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --sdia 0.3 --spdia 0.6 --n_dsgc 1 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.1 --n_dsgc 0 --stimtype 6 --irad1 50 --orad1 75 --sbac_vhold -0.07 --stimtime 0.5 --scontrast 0.012 --minten -0.05 --run_vclamp_sbac 1 --morph_frac 1 --sbac_vpulse -0.07 --sb_denddia 2.2 --predur 0.2 --filename x914

