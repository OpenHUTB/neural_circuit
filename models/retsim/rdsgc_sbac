# - - - - - - - - - - - 
#  run several annuli with Ca chans, then without, then with small g_dbp1_sbac
#
rrdsgc_sbac --filenum 930

rrdsgc_sbac --catmid 0 --catdist 0 --filenum 950

rrdsgc_sbac --g_dbp1_sbac 1e-14 --mglur2 1 --sbac_vpulse 0.0 --filenum 970

rrdsgc_sbac --filenum 930 --makepdf 1
rrdsgc_sbac --catmid 0 --catdist 0 --filenum 950 --makepdf 1
rrdsgc_sbac --g_dbp1_sbac 1e-14 --mglur2 1 --sbac_vpulse 0.0 --filenum 970 --makepdf 1

rrdsgc_sbac --g_dbp1_sbac 1e-14 --mglur2 1 --sbac_vpulse -0.01 --filenum 990
rrdsgc_sbac --g_dbp1_sbac 1e-14 --mglur2 1 --sbac_vpulse -0.01 --filenum 990 --makepdf 1

rrdsgc_sbac --g_dbp1_sbac 1e-14 --mglur2 1 --sbac_vpulse -0.02 --filenum 1010
rrdsgc_sbac --g_dbp1_sbac 1e-14 --mglur2 1 --sbac_vpulse -0.02 --filenum 1010 --makepdf 1


- - - - - - 
# run with small but nonzero d_dbp1_sbac to show on trace

rrdsgc_sbac --g_dbp1_sbac 1e-12 --mglur2 1 --sbac_vpulse 0.0 --filenum 1030
rrdsgc_sbac --g_dbp1_sbac 1e-12 --mglur2 1 --sbac_vpulse 0.0 --filenum 1030 --makepdf 1

- - - - - -
#  try different contrasts with moving annulus, velocity 1000
#   mglur2 0
#   predur 0.5 to get better equilib
#
rrdsgc_sbac_cont --mglur2 0 --filenum 1050 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 1 --filenum 1060 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 2 --filenum 1070 --comnd_line_only 0

# m 0 gives too much regeneration, set lower catdist
#
rrdsgc_sbac_cont --mglur2 0 --filenum 1080 --catdist 3e-3 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 1 --filenum 1090 --catdist 3e-3 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 2 --filenum 1100 --catdist 3e-3 --comnd_line_only 0

rrdsgc_sbac_cont --mglur2 0 --filenum 1110 --catdist 3e-3 --stimtype 1 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 1 --filenum 1120 --catdist 3e-3 --stimtype 1 --comnd_line_only 0

# velocity 2000
#
rrdsgc_sbac_cont --mglur2 0 --filenum 1130 --catdist 3e-3 --stimtype 1 --velocity 2000 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 0 --filenum 1140 --catdist 3e-3 --stimtype 2 --velocity 2000 --comnd_line_only 0

# velocity 500
#
rrdsgc_sbac_cont --mglur2 0 --filenum 1150 --catdist 3e-3 --stimtype 1 --barwidth 25 --velocity 500 --comnd_line_only 0
rrdsgc_sbac_cont --mglur2 0 --filenum 1160 --catdist 3e-3 --stimtype 2 --barwidth 25 --velocity 500 --comnd_line_only 0

# looks like dsi is not good with catmid 5e-3, catdist 3e-3
#
rrdsgc_sbac_cont --mglur2 0 --filenum 1170 --catmid 5e-3 --catdist 6e-3 --stimtype 1 --barwidth 30 
