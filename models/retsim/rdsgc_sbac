# g_sbac_sbac 1e-10 sb1mul 0.3
#
 retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.02 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7_500.s --run_vclamp 1 --g_sbac_dsgc 1e-12 --g_am_dsgc 2.2e-10 --g_am2_dsgc 2.2e-10 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.3 --vhold 0 -v | vid

# am, am2 biophys 1, amca, am2ca 0.2e-3
#
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.1 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7_500.s --run_vclamp 1 --g_sbac_dsgc 1e-12 --g_am_dsgc 4.2e-10 --g_am2_dsgc 4.2e-10 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.3 --vhold 0 --amca 0.2e-3 --am2ca 0.2e-3 -v | vid

#
#  test 13 sbac -> dsgc with am, am2 -> dsgc
#
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 113 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_13_500.s --run_vclamp 1 --g_sbac_dsgc 1e-10 --g_am_dsgc 4.2e-10 --g_am2_dsgc 4.2e-10 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.3 --vhold 0 --amca 0.2e-3 --am2ca 0.2e-3 --predur 0.1 >& dsgc_sbac_13_500.r

# without am, am2
# with noise sbac->sbac, sb1mul 1, thresh dbp1->sbac -0.047
#
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 113 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_13_500.s --run_vclamp 1 --g_sbac_dsgc 1e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 1 --vhold 0 --amca 0.2e-3 --am2ca 0.2e-3 --predur 0.2 >& dsgc_sbac_13_500_0_0.r

# slower to see equilibrium of sbacs
#
mosrun -l retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 500 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 113 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_13_500.s --run_vclamp 1 --g_sbac_dsgc 1e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.8 --vhold 0 --amca 0.2e-3 --am2ca 0.2e-3 --predur 0.2 > dsgc_sbac_13_500_0_0_500.r &

# set lower am2ca, capump to make am2 slower, no sbac->dsgc
#
mosrun -l retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 113 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_13_500.s --run_vclamp 1 --g_sbac_dsgc 1e-14 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-10 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.8 --vhold 0 --amca 0.2e-3 --am2ca 0.05e-3 --predur 0.01 >& dsgc_sbac_13_500_0c.r &

# no am,am2->dsgc, turn on sbac->dsgc
#
mosrun -l retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 113 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_13_500.s --run_vclamp 1 --g_sbac_dsgc 1e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.8 --vhold 0 --amca 0.2e-3 --am2ca 0.05e-3 --predur 0.01 >& dsgc_sbac_13_500_0d.r &

# - - - - - - - - - - - - - - - -
#
# 7 sbacs, no am,am2->dsgc, turn on sbac->dsgc
