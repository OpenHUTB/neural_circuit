# voltage clamp with stepped pulse voltages
#
# mglur1 0
#
rdsgc_sbac_r --sbac_r 0 --g_dbp1_sbac 0.4e-10 --camid 0.0e-3 --catmid 6e-3 --catdist 4e-3 --g_sbac_sbac 2e-10 --sbac_synspac 16 --sbac_maxsdist 5 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.052 --mglur2 0 --sbac_rnd 0 --rnd 35310 --sdia 0.3 --spdia 0.6 --n_dsgc 0 --make_sbac_sbac 0 --make_sbac_dsgc 0 --n_dsgc 0 --n_dbp1 0 --stimtype 7 --sbac_vpulse_dur 0.1 --stimtime 0.1 --scontrast 0.012 --minten -0.05 --sbaclm 0.2 --run_vclamp_sbac 1 --morph_frac 1 --sb_denddia 2.2 --predur 0.2 --filename z10

# cesium 1
#
rdsgc_sbac_r --sbac_r 0 --g_dbp1_sbac 0.4e-10 --camid 0.0e-3 --catmid 6e-3 --catdist 4e-3 --g_sbac_sbac 2e-10 --sbac_synspac 16 --sbac_maxsdist 5 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.052 --mglur2 0 --cesium 1 --sbac_rnd 0 --rnd 35310 --sdia 0.3 --spdia 0.6 --n_dsgc 0 --make_sbac_sbac 0 --make_sbac_dsgc 0 --n_dsgc 0 --n_dbp1 0 --stimtype 7 --sbac_vpulse_dur 0.1 --stimtime 0.1 --scontrast 0.012 --minten -0.05 --sbaclm 0.2 --run_vclamp_sbac 1 --morph_frac 1 --sb_denddia 2.2 --predur 0.2 --filename z11


