#
rrdsgc_sbac_mask --comnd_line_only 1

# to display stimulus only
#
rrdsgc_sbac_mask --d 16 --v 1 --B 0 --n 1 --comnd_line_only 0

# - - - - - - - - - - -
#
#   scontrast series 0.006 - 0.008
#
rrdsgc_sbac_mask  --cont 1 --filenum 50 --comnd_line_only 1


# move stim over limited distance rloc = 0.5
#  display stimulus only
#
rrdsgc_sbac_mask  --cont 4 --filenum 0 --rloc 0.5 --d 16 --v 1 --B 0 --n 1 --comnd_line_only 1

# runs faster rloc 0.3
#
rrdsgc_sbac_mask  --cont 4 --filenum 0 --rloc 0.3 --d 16 --v 1 --B 0 --w 1.4 --n 1 --comnd_line_only 1

# rloc 0.3, cont 4
#
rrdsgc_sbac_mask  --cont 4 --filenum 30 --rloc 0.3 --comnd_line_only 1

# predur 0.1, stimtime 0.01, poststimdur 0.05 
#  display stimulus
#
rrdsgc_sbac_mask --cont 4 --filenum 0 --rloc 1 --d 16 --v 1 --B 0 --n 1 --w 1.4 --comnd_line_only 0


