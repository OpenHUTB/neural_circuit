# run, note that biophys can be set off in command line
#  savefile
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --sbac_densfile dens_sbac_bar.n --n_dsgc 1 --minten -0.043 --sinten 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --scontrast 0.005 --mxrot 0 -d 0 --predur 0.01 --sb_biophys 0 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_savefile dsgc_sbac_7.s -v | vid

# run, note that biophys can be set off in command line
#  restorefile
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --sbac_densfile dens_sbac_bar.n --n_dsgc 1 --minten -0.043 --sinten 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --scontrast 0.005 --mxrot 0 -d 0 --predur 0.01 --sb_biophys 0 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7.s -v | vid

# run, with biophys set off, run_vclamp 1
#   use restorefile
#
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --sbac_densfile dens_sbac_bar.n --n_dsgc 1 --minten -0.040 --sinten 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --scontrast 0.005 --mxrot 0 -d 0 --predur 0.02 --sb_biophys 0 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7.s --run_vclamp 1 -d 0 -v | vid

# run, with biophys set off, run_vclamp 1
#   use restorefile
#   run_vclamp 1, g_am_dsgc 
#
retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --sbac_densfile dens_sbac_bar.n --n_dsgc 1 --minten -0.040 --sinten 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --scontrast 0.005 --mxrot 0 -d 0 --predur 0.02 --sb_biophys 0 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7.s --run_vclamp 1 --g_am_dsgc 2.2e-10 --g_am2_dsgc 2.2e-10 -d 0 --vhold 0 -v | vid 

