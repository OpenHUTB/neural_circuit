mosrun -l retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7_500.s --run_vclamp 1 --g_sbac_dsgc 1e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.5 --vhold 0 --amca 0.2e-3 --am2ca 0.05e-3 --predur 0.01 >& dsgc_sbac_7_500_0_0.r &

mosrun -l retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_168c4 --n_dsgc 1 --minten -0.040 --scontrast 0.005 --velocity 2000 --endwait 0.2 --no_excit 1 --no_inhib 0 --movein -1 --ninfo 2 --sdia 0.2 --sbarr 107 --make_sbac_sbac 1 --sbac_mouse 1 --sbac_nscale -2.05 --stimtype 1 --mxrot 0 -d 0 --predur 0.05 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --syn_restorefile dsgc_sbac_7_500.s --run_vclamp 0 --g_sbac_dsgc 2e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --g_sbac_sbac 1e-10 -d 0 --dtreedia 500 --sb1mul 0.5 --vhold 0 --amca 0.2e-3 --am2ca 0.05e-3 --predur 0.01 > & dsgc_sbac_7_v500_0_0.r &

