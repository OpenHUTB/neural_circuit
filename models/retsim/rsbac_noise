mosrun -l retsim --expt sbac_noise --sbac_file morph_R1MS151208_06c --soma_z -31 --minten -0.038 --econtrast 0.004 --eincr 0 --icontrast 0.017 --iincr 0.00417 --velocity 440 --predur 0.1 --prestimdur 0.05 --poststimdur 0.05 --vhold -0.05 --vstart -0.070 --vstop -0.07 --sb_vs -0.045 --sb_vr -0.07 --dsomadia 10 --sdia 0.6 --spdia_ratio 0.9 --sb_rm 9.257e3 --dri 40 --drm 9.257e3 --dcm 0.86e-6 --elec_rs 19.3e6 --elnode 5000 --light_inhib 1 --ampa_cond 4e-09 --gaba_cond 4.60799e-09 --movein -1 --set_vclamp 1 --ttxbath 1 --tea 0.995 --kdr_voff 0.027 --kexp 3.6 --fourap 1 --ioffset 0 --use_ghki 1 --dbp1_anpo 100 --dbp1_anpi 10 --ninfo 2 --stimscale 0.3 --n_ams 0 --n_dbp2 0 --plotlabel sbac_noise_800_180 --exptrun 6 --stimtype 7 --dbp1_sgain 6 --dbp1_vgain 8 --sb_rmd 20e3 --catdist 1.2e-3 --ksoma 2.5e-3 --kprox 2e-3 --kmid 2e-3 --kdist 2e-3 --datfile KO_cell_1_scaled.txt --vscale 1e-3 --sb_vr -0.069 --nbase -0.069 --n_dsgc 1 --sbmaxrate 20 --nscale 1 --mxrot 0 --predur 0.02 --ppdelay 0.05 --sb_cn 1 --sbac_isynrngi -1300 --sbac_dens 500 --make_sbac_sbac 1 --sbsynang 180 --arrsiz 800 --syn_savefile sbac_noise_800_180.s -d 1 -v | vid -c > sbac_noise_800_180b.ps

