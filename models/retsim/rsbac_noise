#  Ca chans cause regeneration
#
mosrun -l retsim --expt sbac_noise --n_sbac 1 --sbac_file morph_R1MS151208_06b --soma_z -31 --minten -0.038 --econtrast 0.004 --eincr 0 --icontrast 0.017 --iincr 0.00417 --velocity 440 --predur 0.1 --prestimdur 0.05 --poststimdur 0.005 --vhold -0.07 --vstart -0.070 --vstop -0.07 --sb_vs -0.065 --sb_vr -0.07 --dsomadia 10 --sdia 0.6 --spdia_ratio 0.9 --sb_rm 9.257e3 --dri 23.22 --drm 9.257e3 --dcm 0.86e-6 --elec_rs 19.3e6 --elnode 5000 --light_inhib 1 --ampa_cond 4e-09 --gaba_cond 4.60799e-09 --movein -1 --set_vclamp 1 --ttxbath 1 --tea 0.995 --kdr_voff 0.027 --kexp 3.6 --fourap 1 --ioffset 0 --use_ghki 1 --dbp1_anpo 100 --dbp1_anpi 10 --ninfo 2  --stimscale 0.3 --n_ams 0 --n_dbp2 0 --_COL 1000 --plotlabel plot9 --exptrun 2 --stimtype 7 --dbp1_sgain 6 --dbp1_vgain 8 --sb_rmd 20e3 --cadist 1e-3 -d 0 >& sbac_noise_plot9.r &

