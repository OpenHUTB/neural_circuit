rdsgc_sbac_r --sbac_file morph_sbac_ti_12_10_20 --sbac_r 60 --sb_arbscale 1.0 --dri 100 --spdia 0.4 --sdia 0.17 --sddia 0.7 --g_dbp1_sbac 10e-10 --camid 0.0e-3 --cadist 0 catprox 0.2e-3 --catmid  3.5e-3 --catdist 7.5e-3 --g_sbac_sbac 0.3e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 0 --sbaclm 0.05 --stimtype 1  --scontrast 0.004 --minten -0.049 --sb_ca6_offm 0.026 --sb_cap 8e-7 --velocity 600 --predur 0.2 --poststimdur 0.2 --n_dbp1_sbac 0 --dbp1_mrate 500 --dbp1_mpool 250 --barwidth 20 --sbxarrsiz 300 --sbyarrsiz 250 --sbac_dens 800 --sb_vs -0.062 --n_dbp1 0 --d 1 


