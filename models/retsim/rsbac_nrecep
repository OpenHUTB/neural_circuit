
# display the cell
#
rdsgc_sbac_r --sbac_file morph_sbac_ti_12_10_20 --sbac_r 0 --dri 100 --spdia 0.4 --sdia 0.17 --sddia 0.8 --g_dbp1_sbac 15e-10 --camid 0.0e-3 --cadist 0 --catprox 0.2e-3 --catmid 3.5e-3 --catdist 7.5e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 0 --sbaclm 0.02 --stimtype 1 --scontrast 0.004 --minten -0.05 --sb_ca6_offm 0.026 --sb_capp 2e-7 --sb_capm 3e-7 --sb_cap 6e-7 --cadiap 1.0 --cadiam 1.0 --cadiad 1.0 --velocity 600 --predur 1.0 --poststimdur 0.5 --n_dbp1_sbac 0 --dbp1_mrate 500 --dbp1_mpool 500 --barwidth 17 --mxrot 180 --plotlabel morph_sbac_ti_12_10_20 --d 1

# display the cell with dbp1s and dbp2s
#
rdsgc_sbac_r --sbac_file morph_sbac_ti_12_10_20 --sbac_r 0 --dri 100 --spdia 0.4 --sdia 0.17 --sddia 0.8 --g_dbp1_sbac 15e-10 --camid 0.0e-3 --cadist 0 --catprox 0.2e-3 --catmid 3.5e-3 --catdist 7.5e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 0 --sbaclm 0.02 --stimtype 1 --scontrast 0.004 --minten -0.05 --sb_ca6_offm 0.026 --sb_capp 2e-7 --sb_capm 3e-7 --sb_cap 6e-7 --cadiap 1.0 --cadiam 1.0 --cadiad 1.0 --velocity 600 --predur 1.0 --poststimdur 0.5 --n_dbp1_sbac 0 --dbp1_mrate 500 --dbp1_mpool 500 --barwidth 17 --mxrot 180 --sb_arbscale 1 --plotlabel morph_sbac_ti_12_10_20 --make_dbp2_sbac 1 --dbp1_maxsdist 7 --dbp1_dens 10000 --dbp2_dens 10000 --d 1 --v 1

