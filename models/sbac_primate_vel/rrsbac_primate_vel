# - - - - - - - - - - - - - - - -
#
#  similar to y124i, y125i, but velocity 1000 barwidth 50, catsoma 0.2e-3, catprox 0.2e-3, catmid 3.5e-3, catdist 7.5e-3
#   stim_fnum 570
#
# run with sustained bipolars
#   db1_cap, db2_cap 4e-6, db1_ca6ax 0.5e-3, db2_ca6ax 0.5e-3 
#   naab 0, kax 0e-3, g_dbp2_sbac 5e-10
#   db1_ca6_offm, db2_ca6_offm 0.02 in rsbac_primate_vel
#   db1_lam, db2_lam 0.05 in rsbac_primate_vel (for dens_midg_primate.n, dens_db4_primate.n )
#
rsbac_primate_vel --sbarr 0 --dri 100 --velocity 1000 --scontrast 0.0055 --stimtype 13 --minten -0.056 --db1_cap 4e-6 --db2_cap 4e-6 --db1_ca6ax 0.5e-3 --db2_ca6ax 0.5e-3 --db2_ca6_tauc 50 --db2_ca6_taud 50 --naab 0e-3 --kax 0e-3 --ksoma 0 --kprox 0 --rndv 55700 --rnd_n 30 --dbp2_mrate 1000 --dbp2_mpool 15 --barwidth 50 --barlength 500 --g_dbp1_sbac 5e-10 --g_dbp2_sbac 5e-10 --dbp1_mpool 15 --dbp1_mrate 1000 --g_dbp_scal 1.0 --nosbca 0 --dbp1_dens 2300 --dbp2_dens 800 --db1_r6 0.2 --db2_r6 1 --dbp2_maxsdist 10 --set_sr 16 --catsoma 0.2e-3 --catprox 0.2e-3 --catmid 3.5e-3 --catdist 7.5e-3 --surr_delay 0.01 --surr_weight -0.7 --surr_tau 0 --sbac_densfile dens_sbac_bar4a_db4.n --use_stimfile 1 --makestim 0 --stim_fnum 570 --file_chars y142i --filenum 7000

# run with transient bipolars
#   db1_cap, db2_cap 2e-6, db1_ca6ax 0.5e-3, db2_ca6ax 0.2e-3
#   naab 2000e-3, kax 1e-3, g_dbp2_sbac 5e-10, dbp2_mrate 200
#   db1_ca6_offm, db2_ca6_offm 0.02 in rsbac_primate_vel
#
