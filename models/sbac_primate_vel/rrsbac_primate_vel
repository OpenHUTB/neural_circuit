rsbac_primate_vel --sbarr 0 --velocity 1000 --scontrast 0.007 --stimtype 13 --minten -0.052 --db1_cap 8e-6 --db2_cap 8e-6 --db1_caax 5e-3 --db2_caax 8e-3 --rnd_n 1 --dbp2_mrate 50 --dbp2_mpool 15 --barwidth 200 --g_dbp1_sbac 3e-10 --g_dbp2_sbac 10e-10 --dbp1_mrate 400 --g_dbp_scal 0.7 --filenum 160

