rsbac_primate_vel --sbarr 0 --dri 100 --velocity 2000 --scontrast 0.007 --stimtype 13 --minten -0.053 --db1_cap 8e-6 --db2_cap 8e-6 --db1_caax 5e-3 --db2_caax 8e-3 --rndv 2073 --rnd_n 4 --dbp2_mrate 50 --dbp2_mpool 15 --barwidth 100 --g_dbp1_sbac 3e-10 --g_dbp2_sbac 5e-10 --dbp1_mpool 15 --dbp1_mrate 1000 --g_dbp_scal 2.5 --nosbca 1 --db1_r6 1 --db2_r6 0 --file_chars nf --filenum 9700 --comnd_line_only 1

