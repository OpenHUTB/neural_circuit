# make .ps file to be able to edit
#
retsim --expt dsgc_sbac_bar --sbac_file morph_sbac_168c6 --dsomadia 8 --dri 100 --spdia 0.3 --sdia 0.2 --sddia 0.8 --dbp2_maxsdist 10 --dbp1_dens 10000 --dbp2_dens 4600 --sbac_soma_z -45 --sb_arbscale 1 --sb_denddia 1 --n_dsgc 0 --sbarr 0 --minten -0.056 --scontrast 0.007 --endwait 0.2 --ninfo 2 --make_sbac_sbac 1 --sbac_species 1 --sbac_nscale -2.05 -d 0 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --run_vclamp 0 --dtreedia 500 --predur 0.5 --sbac_r 180 --dsgc_x -150 --mxrot 0 --myrot 0 --mzrot 0 --sb1mul 2 --vhold 0 --camid 0 --cadist 0 --catsoma 0 --catprox 0 --catmid 0 --catdist 0 --amca 0.0002 --am2ca 5e-05 --dscavg 100000 --dscaeg 1 --g_dbp1_dsgc 5e-10 --g_dbp1_sbac 1.5e-10 --g_sbac_dsgc 1e-09 --g_sbac_sbac 2e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --set_ploti 0.001 --n_dbp1_sbac 0 --v_dbp1_sbac 3 --mglur2 0 --barwidth 200 --velocity 200 --stimtype 13 --poststimdur 0.2 --n_am 0 --n_am2 0 --dbpthr -0.05 --sbac_rnd 1 -r 55702 --make_sbac_dsgc 0 --sbaclm 0.02 --ksoma 0 --kprox 0 --disp_stim_max -0.04 --disp_stim_min -0.05 --disp_calib_x 0.9 --disp_calib_y 0.22 --stim_fnum 555 --use_stimfile 1 --makestim 0 --sb_cap 6e-07 --sb_capm 3e-07 --sb_capp 2e-07 --dbp1_mrate 1000 --dbp1_mpool 15 --surr_delay 0.01 --surr_tau 0 --surr_weight -0.7 --make_dbp2_sbac 1 --dbp2thr -0.05 --dbp2_mrate 1000 --dbp2_mpool 15 --g_dbp2_sbac 1.5e-10 --n_dbp2_sbac 0 --v_dbp2_sbac 1.7 --db1_r5 0.1 --db2_r5 0.5 --db1_r6 0.3 --db2_r6 1 --draw_synapse 3 --sbac_densfile dens_sbac_bar5c.n --db1_lam 0.05 --db1_morph 4 --db1_sca 1 --db2_lam 0.05 --db2_morph 4 --db2_sca 1 --db_vs -0.056 --naab 0 --kax 0 --db1_ca1_offm 0.005 --db1_ca6_offm 0.02 --db2_ca1_offm 0.005 --db2_ca6_offm 0.02 --db2_ca6_tauc 50 --db2_ca6_taud 50 --db1_ca1ax 0 --db1_ca6ax 0.0005 --db2_ca1ax 0 --db2_ca6ax 0.0005 --db1_cap 4e-06 --db2_cap 4e-06 --set_sr 16 --plotlabel sbac_vel_db2_0_180_55702_13_0.007_200_200_y46m7002 -D -d 1 -v | vid -B 7 -c > sbac_y46m7002b.ps

