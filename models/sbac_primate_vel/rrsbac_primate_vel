# - - - - - - - - - - - - - -
#
# vclamp series, stimtype 7
#  get ca6 activation curve
#  default catprox, catmid, catdist
#
rsbac_primate_vel --sbarr -2 --velocity 1000 --scontrast 0.007 --stimtype 7 --minten -0.052 --db1_cap 8e-6 --db2_cap 8e-6 --db1_caax 5e-3 --db2_caax 8e-3 --rndv 2073 --rnd_n 1 --dbp2_mrate 50 --dbp2_mpool 15 --barwidth 50 --g_dbp1_sbac 3e-10 --g_dbp2_sbac 5e-10 --dbp1_mpool 15 --dbp1_mrate 1000 --n_dbp1 0 --n_dbp2 0 --g_dbp_scal 1.1 --db1_r6 0.3 --db2_r6 1 --sb_ca6_offm 0.015 --run_vclamp_sbac 1 --vstart -0.07 --vstop -0.02 --vstep 0.005 --ivplot 0 --sbac_densfile2 dens_sbac_bar3_db4.n --file_chars vclamp --filenum 1

