#  make db2 conductance greater because it is more transient
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_species 3 --sbac_r 135 --g_dbp1_sbac 15.0e-10 --camid 0.0e-3 --cadist 0 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --mglur2 0 --n_dsgc 0 --dscavg 1e5 --rnd 35317 --make_sbac_sbac 0 --stimtype 15 --scontrast 0.007 --minten -0.054 --predur 0.5 --poststimdur 0.02 --tea 0 --tfreq 2 --ncycles 4 --twaveshape 0 --db1_caax 1e-3 --db1_ca6_offm 0.028 --db2_ca6_offm 0.028 --g_dbp2_sbac 30e-10 --n_dbp2_sbac 0 --nahd 0e-3 --naab 100e-3 --kax 0.3e-3 --db2_caax 1e-3 --na5offs 0 --k1offm 0.015 --k6offm 0.015 --dbp1_mrate 200 --dbp1_mpool 50 --dbp2_mrate 50 --dbp2_mpool 8 --blur_ssize 120 --surr_weight -0.9 --surr_delay 0.08 --surr_tau 0 --velocity 200 --swaveshape 0 --barwidth 400 --dbp1_dens 1900 --dbp2_dens 900 --stonic 1006 --db1_r6 0.3 --db2_r6 1 --ksoma 3e-3 --kprox 2e-3 --kmid 1e-3 --catprox 2e-3 --catmid 5e-3 --catdist 7e-3 --naprox 0 --namid 0 --nadist 0 --sb_capm 1e-6 --sb_cap 2e-6 --sb_ca6_tauc 8 --sb_ca6_taud 1 --disp_stim_min -0.065 --disp_stim_max -0.042 --use_stimfile 1 --makestim 0 --stim_fnum 278 --filename d412d

#  same as d412, with different stimulus file
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_species 3 --sbac_r 135 --g_dbp1_sbac 15.0e-10 --camid 0.0e-3 --cadist 0 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --mglur2 0 --n_dsgc 0 --dscavg 1e5 --rnd 35317 --make_sbac_sbac 0 --stimtype 15 --scontrast 0.007 --minten -0.054 --predur 0.5 --poststimdur 0.02 --tea 0 --tfreq 2 --ncycles 4 --twaveshape 0 --db1_caax 1e-3 --db1_ca6_offm 0.028 --db2_ca6_offm 0.028 --g_dbp2_sbac 15e-10 --n_dbp2_sbac 0 --nahd 0e-3 --naab 100e-3 --kax 0.3e-3 --db2_caax 1e-3 --na5offs 0 --k1offm 0.015 --k6offm 0.015 --dbp1_mrate 200 --dbp1_mpool 50 --dbp2_mrate 50 --dbp2_mpool 8 --blur_ssize 120 --surr_weight -0.9 --surr_delay 0.08 --surr_tau 0 --velocity 200 --swaveshape 0 --barwidth 400 --dbp1_dens 1900 --dbp2_dens 900 --stonic 1006 --db1_r6 0.3 --db2_r6 1 --ksoma 3e-3 --kprox 2e-3 --kmid 1e-3 --catprox 2e-3 --catmid 5e-3 --catdist 7e-3 --naprox 0 --namid 0 --nadist 0 --sb_capm 1e-6 --sb_cap 2e-6 --sb_ca6_tauc 8 --sb_ca6_taud 1 --disp_stim_min -0.065 --disp_stim_max -0.042 --use_stimfile 1 --makestim 0 --stim_fnum 278 --filename d412e

[[ original stim_fnum 269 got overwritten, so use 278 instead ]]

plotmod -p 1-2 -p 24 d412c.r | vidpdf > d412c.pdf    (dbp2s replaced with dbp1s)
plotmod -p 1-2 -p 29 d412d.r | vidpdf > d412d.pdf    (greater dbp2 conductance because it is more transient)
plotmod -p 1-2 -p 29 d412e.r | vidpdf > d412e.pdf    (original with dbp1s and dbp2s)

