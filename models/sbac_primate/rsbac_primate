# - - - - - - - - - 
#
# stimtype 12, test of spot with d-o-g center / surround
#   blur_ssize 60 to decrease time to calc
#   predur 0.5
#   tea 1 (no K chans), and catmid, catdist 0 (no Ca chans)
#   center weight 1, surr_weight -1
#   scontrast 0.005, minten -0.05
#   to set spot interval, set spotint
#   to set number of cycles, set ncycles
#   to set dia increment, set spotdia_incr
#   to make annulus, set annulus_width = 50 
#   to make annulus outer dia constant, set annulus_odia > 0
#   to add negative spot after pos spot is done, set scontrastn > 0
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.005 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 60 --surr_delay 0.02 --ncycles 4 --velocity 2000 --swaveshape 0 --spotdia 100 --dbp1_mpool 200 --dbp1_mrate 2000 --tea 1 --center_weight 1 --surr_weight -1 --spotint 0.1 --spotdur 0.05 --use_stimfile 1 --makestim 1 --stim_fnum 58 &

# blur_ssize 60, annulus_width 50
#
