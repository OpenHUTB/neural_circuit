# - - - - -
#  same as d5405c4, but center weight 0.001
#  1 Hz
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_species 3 --sbac_r 135 --g_dbp1_sbac 15.0e-10 --camid 0.0e-3 --cadist 0 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --mglur2 0 --n_dsgc 0 --dscavg 1e5 --rnd 35317 --make_sbac_sbac 0 --stimtype 15 --scontrast 0.007 --minten -0.054 --predur 0.5 --poststimdur 0.02 --tea 0 --tfreq 2 --ncycles 4 --twaveshape 1 --db1_caax 1e-3 --db1_ca6_offm 0.028 --db2_ca6_offm 0.028 --g_dbp2_sbac 15e-10 --n_dbp2_sbac 0 --nahd 0e-3 --naab 100e-3 --kax 0.3e-3 --db2_caax 1e-3 --na5offs 0 --k1offm 0.015 --k6offm 0.015 --dbp1_mrate 200 --dbp1_mpool 50 --dbp2_mrate 50 --dbp2_mpool 8 --blur_ssize 120 --center_weight 0.001 --surr_weight -0.9 --surr_delay 0 --surr_tau 0 --velocity 500 --swaveshape 1 --barwidth 500 --dbp1_dens 1900 --dbp2_dens 900 --stonic 1006 --db1_r6 0.3 --db2_r6 1 --ksoma 3e-3 --kprox 2e-3 --kmid 1e-3 --catprox 2e-3 --catmid 5e-3 --catdist 7e-3 --naprox 0 --namid 0 --nadist 0 --sb_capm 1e-6 --sb_cap 2e-6 --sb_ca6_tauc 8 --sb_ca6_taud 1 --disp_stim_min -0.065 --disp_stim_max -0.042 --use_stimfile 1 --makestim 1 --stim_fnum 282 --filename d405c5 &

plotmod -p 1 -t d405c4.r  | remcomm > csnd1.txt
plotmod -p 1 -t d408.r    | remcomm | column 2 > cnd1.txt
plotmod -p 1 -t d405c5.r  | remcomm | column 2 > snd1.txt
hc csnd1.txt  cnd1.txt > csnd1b.txt
hc csnd1b.txt snd1.txt > csnd1c.txt
plotmod csnd1c.txt | vidpdf > csnd1c.pdf

plotmod -p 2 -t d405c4.r  | remcomm > csnd2.txt
plotmod -p 2 -t d408.r    | remcomm | column 2 > cnd2.txt
plotmod -p 2 -t d405c5.r  | remcomm | column 2 > snd2.txt
hc csnd2.txt  cnd2.txt > csnd2b.txt
hc csnd2b.txt snd2.txt > csnd2c.txt
plotmod csnd2c.txt | vidpdf > csnd2c.pdf

