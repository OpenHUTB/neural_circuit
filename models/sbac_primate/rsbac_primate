# display stim
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 13  --scontrast 0.0045 --minten -0.046 --disp_stim_max -0.043 --disp_stim_min -0.048 --sb_ca6_offm 0.026 --poststimdur 0.2 --n_dbp1_sbac 0 --blur_ssize 80 --use_stimfile 1 --makestim 0 --stim_fnum 42 --d 16 --v 1 --B 0 &

