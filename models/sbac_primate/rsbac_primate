rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_species 3 --sbac_r 135 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid 0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --mglur2 0 --n_dsgc 0 --dscavg 1e5 --rnd 35310 --make_sbac_sbac 0 --stimtype 12 --scontrast 0.005 --minten -0.050 --predur 0.5 --poststimdur 0.02 --tea 0 --tfreq 2 --ncycles 2 --twaveshape 1 --ca7ax 0.3e-3 --db1_ca7_offm 0.028 --db2_ca6_offm 0.028 --g_dbp2_sbac 5e-10 --n_dbp2_sbac 0 --nahd 0e-3 --naab 0e-3 --kax 0.3e-3 --ca6ax 1e-3 --na5offs 0 --k1offm 0.015 --k6offm 0.015 --annulus_odia 0 --spotdia_incr 25 --annulus_width 0 --spotdia_max 300 --use_stimfile 1 --makestim 0 --stim_fnum 187 --filename d241 --db1_r6 0.7 --db2_r6 0.3 --comnd_line_only 1 

# to make dbp2 inputs low density but more regular, set db2_r6 1, dbp2_dens 1700
# 
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_species 3 --sbac_r 135 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid 0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --mglur2 0 --n_dsgc 0 --dscavg 1e5 --rnd 35310 --make_sbac_sbac 0 --stimtype 12 --scontrast 0.005 --minten -0.050 --predur 0.5 --poststimdur 0.02 --tea 0 --tfreq 2 --ncycles 2 --twaveshape 1 --ca7ax 0.3e-3 --db1_ca7_offm 0.028 --db2_ca6_offm 0.028 --g_dbp2_sbac 5e-10 --n_dbp2_sbac 0 --nahd 0e-3 --naab 0e-3 --kax 0.3e-3 --ca6ax 1e-3 --na5offs 0 --k1offm 0.015 --k6offm 0.015 --annulus_odia 0 --spotdia_incr 25 --annulus_width 0 --spotdia_max 300 --use_stimfile 1 --makestim 0 --stim_fnum 188 --filename d241 --db1_r6 0.7 --db2_r6 1 --dbp2_dens 1700 --comnd_line_only 1 

