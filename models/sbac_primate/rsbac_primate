# - - - - - - -
#
# annulus_odia 1000, annulus_width 50
# blur_ssize 120, surr_weight -0.8, tfreq 2, scontrast 0.003
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.003 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --tea 1 --annulus_odia 1000 --annulus_width 50 --center_weight 1 --surr_weight -0.8 --tfreq 5 --ncycles 4 --twaveshape 1 --use_stimfile 1 --makestim 1 --stim_fnum 80 &

rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.003 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --tea 1 --annulus_odia 1000 --annulus_width 50 --center_weight 1 --surr_weight -0.8 --tfreq 5 --ncycles 4 --twaveshape 1 --use_stimfile 1 --makestim 0 --stim_fnum 80 --d 16 --v 1 --B 0

# run
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.003 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --tea 1 --annulus_odia 1000 --annulus_width 50 --center_weight 1 --surr_weight -0.8 --tfreq 5 --ncycles 4 --twaveshape 1 --use_stimfile 1 --makestim 0 --stim_fnum 80 --filename d80

plotmod -e 0.2 -E 0.6 -p 1-17 -t d80.r > r0.r
plotmod -e 1.0 -E 1.4 -p 1-17 -t d80.r > r50.r
plotmod -e 1.8 -E 2.2 -p 1-17 -t d80.r > r100.r
plotmod -e 2.6 -E 3.0 -p 1-17 -t d80.r > r150.r
plotmod -e 3.4 -E 3.8 -p 1-17 -t d80.r > r200.r
plotmod -e 4.2 -E 4.6 -p 1-17 -t d80.r > r250.r


plotmod -e 0.2 -E 0.6 -p 17 -t d80.r | remcomm | rezero > r0_soma
plotmod -e 1.0 -E 1.4 -p 17 -t d80.r | remcomm | rezero > r50_soma
plotmod -e 1.8 -E 2.2 -p 17 -t d80.r | remcomm | rezero > r100_soma
plotmod -e 2.6 -E 3.0 -p 17 -t d80.r | remcomm | rezero > r150_soma
plotmod -e 3.4 -E 3.8 -p 17 -t d80.r | remcomm | rezero > r200_soma
plotmod -e 4.2 -E 4.6 -p 17 -t d80.r | remcomm | rezero > r250_soma

hc r0_soma r50_soma > soma; mv soma soma2
hc soma2 r100_soma  > soma; mv soma soma2
hc soma2 r150_soma  > soma; mv soma soma2
hc soma2 r200_soma  > soma; mv soma soma2
hc soma2 r250_soma  > sbac_soma_rf

plotmod -M 0 sbac_soma_rf | vid
plotmod -M 0 sbac_soma_rf | vidpdf > sbac_soma_rf.pdf


# - - - - - -
# stimtype 12, test of (sine/square) modulated spot with d-o-g center / surround
#  including dbp2, transient bipolar cell input
#  sbarr 0 (nsbac==1)
#
# blur_ssize 60 
#  tfreq 10, ncycles 4, twaveshape 1 (square wave modulation)
#   scontrast 0.005
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.005 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 60 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --dbp2thr -0.052 --sbac_maxsdist 15 --make_dbp2_sbac 1 --g_dbp2_sbac 20e-10 --n_dbp2_sbac 0 --v_dbp2_sbac 1.9 --dbp2_mrate 30 --dbp2_mpool 6 --dbp1_maxsdist 7 --tea 1 --center_weight 1 --surr_weight -1 --tfreq 10 --ncycles 4 --twaveshape 1 --use_stimfile 1 --makestim 1 --stim_fnum 81 &

# display stimulus
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.005 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 60 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --dbp2thr -0.052 --sbac_maxsdist 15 --make_dbp2_sbac 1 --g_dbp2_sbac 20e-10 --n_dbp2_sbac 0 --v_dbp2_sbac 1.9 --dbp2_mrate 30 --dbp2_mpool 6 --dbp1_maxsdist 7 --tea 1 --center_weight 1 --surr_weight -1 --tfreq 10 --ncycles 4 --twaveshape 1 --use_stimfile 1 --makestim 0 --stim_fnum 81 --d 16 --v 1 --B 0

# run model
#
# blur_ssize 60 
#  tfreq 20, ncycles 4
#   scontrast 0.005
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.005 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 60 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --dbp2thr -0.052 --sbac_maxsdist 15 --make_dbp2_sbac 1 --g_dbp2_sbac 20e-10 --n_dbp2_sbac 0 --v_dbp2_sbac 1.9 --dbp2_mrate 30 --dbp2_mpool 6 --dbp1_maxsdist 7 --tea 1 --center_weight 1 --surr_weight -1 --tfreq 10 --ncycles 4 --twaveshape 1 --use_stimfile 1 --makestim 0 --stim_fnum 81 --filename d81



