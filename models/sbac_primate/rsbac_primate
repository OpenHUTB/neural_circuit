#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  0e-3 --catdist 0e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 16  --scontrast 0.005 --minten -0.050 --disp_stim_max -0.045 --disp_stim_min -0.052 --sb_ca6_offm 0.026 --predur 0.5 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0.02 --dbp1_mpool 200 --dbp1_mrate 2000 --dbp2thr -0.052 --sbac_maxsdist 15 --make_dbp2_sbac 1 --g_dbp2_sbac 20e-10 --n_dbp2_sbac 0 --v_dbp2_sbac 1.9 --dbp2_mrate 30 --dbp2_mpool 6 --dbp1_maxsdist 7 --tea 1 --center_weight 1 --surr_weight -1 --tfreq 10 --ncycles 2 --nstim 6 --direction 1 --stonic 1001 --swaveshape 1 --velocity 1000 --use_stimfile 0 --makestim 0 --stim_fnum 89 --d 1 --v 1 --B 0

# make stimulus, tfreq 2, ncycles 2
#
