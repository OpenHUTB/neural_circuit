rdsgc_sbac_r --sbac_file morph_sbac_cell5 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12  --scontrast 0.0056 --minten -0.050 --sb_ca6_offm 0.026 --poststimdur 0.2 --n_dbp1_sbac 0 --blur_ssize 60 --disp_stim_max -0.045 --disp_stim_min -0.052 --use_stimfile 1 --makestim 1 

retsim --expt dsgc_sbac_bar --dsgc_file morph_ds1eb --sbac_file morph_sbac_cell5 --dsomadia 8 --dri 200 --spdia 0.46 --sdia 0.37 --sddia 0.64 --dbp1_dens 9500 --sbac_soma_z -45 --sb_arbscale 1 --sb_denddia 1 --n_dsgc 0 --sbarr 0 --minten -0.05 --scontrast 0.0056 --endwait 0.2 --ninfo 2 --make_sbac_sbac 0 --sbac_mouse 1 --sbac_nscale -2.05 --mxrot 0 -d 0 --sb_biophys 1 --dsgc_biophys 0 --am_nscale -3.2 --dsgc_nscale -2.01 --run_vclamp 0 --dtreedia 500 --predur 0.2 --sbac_r 120 --dsgc_x -150 --sb1mul 1 --vhold 0 --camid 0 --cadist 0 --catsoma 0 --catprox 0 --catmid 0.004 --catdist 0.006 --amca 0.0002 --am2ca 5e-05 --dscavg 100000 --g_dbp1_dsgc 5e-10 --g_dbp1_sbac 5e-10 --g_sbac_dsgc 1e-09 --g_sbac_sbac 2e-10 --g_am_dsgc 4.2e-14 --g_am2_dsgc 4.2e-14 --set_ploti 0.001 --sb_ca6_offm 0.026 --n_dbp1_sbac 0 --mglur2 0 --stimtype 1 --poststimdur 0.2 --n_am 0 --n_am2 0 --dbpthr -0.05 --sbac_rnd 0 -r 35310 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 12 --scontrast 0.025 --disp_stim_max -0.045 --disp_stim_min -0.052 --blur_ssize 60 --remove_nconns 0 --use_stimfile 1 --makestim 0 -d 16 -v | vid

