# - - - - - - - - - - - - - - -
#
# use_stimfile 1, makestim 1
#  minten -0.046, scontrast 0.005
#
#  stimtype 15 (expanding/contracting circular sinewave grating: "sineann")
#  surr_delay 0, ncycles 3, velocity 2000, 
#  barwidth 100 (spatial period: 1 cycle = 100 um)
#  waveshape 0 (sine) 
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 15  --scontrast 0.005 --minten -0.047 --disp_stim_max -0.040 --disp_stim_min -0.050 --sb_ca6_offm 0.026 --poststimdur 0.2 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0 --ncycles 3 --velocity 2000 --swaveshape 0 --barwidth 100 --use_stimfile 1 --makestim 1 --stim_fnum 50 &

# display stim
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 15  --scontrast 0.005 --minten -0.047 --disp_stim_max -0.040 --disp_stim_min -0.050 --sb_ca6_offm 0.026 --poststimdur 0.2 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0 --ncycles 3 --velocity 2000 --swaveshape 0 --barwidth 100 --use_stimfile 1 --makestim 0 --stim_fnum 50 --d 16 --v 1 --B 0 &

# run model
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 15  --scontrast 0.005 --minten -0.047 --disp_stim_max -0.040 --disp_stim_min -0.050 --sb_ca6_offm 0.026 --poststimdur 0.2 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0 --ncycles 3 --velocity 2000 --swaveshape 0 --barwidth 100 --use_stimfile 1 --makestim 0 --stim_fnum 50 --filename d20 &

# to show bipolar cell responses:
#
plotmod -p 8-14 -M 100 -E 0.6  d20.r | vid


# - - - - - - - - - - 
#  stimtype 15
#
#  turn off synaptic depletion: 
#     dbp1_mpool 200, dbp1_mrate 1000 (from default dbp1_mpool 100, dbp1_mrate 25
#     turn on sbac->sbac inhibition: make_sbac_sbac 1
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 15  --scontrast 0.005 --minten -0.047 --disp_stim_max -0.040 --disp_stim_min -0.050 --sb_ca6_offm 0.026 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0 --ncycles 4 --velocity 2000 --swaveshape 0 --barwidth 100 --dbp1_mpool 200 --dbp1_mrate 2000 --use_stimfile 1 --makestim 1 --stim_fnum 50 &

# display stim
#
rdsgc_sbac_r --sbac_file morph_sbac_dd_cell2 --sbac_r 120 --sb_arbscale 1 --g_dbp1_sbac 5.0e-10 --camid 0.0e-3 --cadist 0 --catmid  4e-3 --catdist 6e-3 --g_sbac_sbac 2e-10 --g_sbac_dsgc 10e-10 --sbarr 0 --dbpthr -0.050 --mglur2 0 --n_am 0 --n_am2 0 --n_dsgc 0 --dscavg 1e5 --sbac_rnd 0 --rnd 35310 --make_sbac_sbac 0 --make_sbac_dsgc 1 --sbaclm 0.05 --stimtype 15  --scontrast 0.005 --minten -0.047 --disp_stim_max -0.040 --disp_stim_min -0.050 --sb_ca6_offm 0.026 --poststimdur 0.02 --n_dbp1_sbac 0 --blur_ssize 120 --surr_delay 0 --ncycles 4 --velocity 2000 --swaveshape 0 --barwidth 100 --dbp1_mpool 200 --dbp1_mrate 2000 --use_stimfile 1 --makestim 0 --stim_fnum 50 --d 16 --v 1 --B 0 

