# run model
#  istim 50e-12, dsgc_denddia 1.0
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 1.0 > dend_dive_1.r &

#  dsgc_denddia 0.8
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 0.8 > dend_dive_2.r &

# dsgc_denddia 0.5
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 0.5 > dend_dive_3.r &

# dsgc_denddia 1.2
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 1.2 > dend_dive_4.r &


plotmod dend_dive_1.r | vidpdf > dend_dive_1.pdf
plotmod dend_dive_2.r | vidpdf > dend_dive_2.pdf
plotmod dend_dive_3.r | vidpdf > dend_dive_3.pdf
plotmod dend_dive_4.r | vidpdf > dend_dive_4.pdf


