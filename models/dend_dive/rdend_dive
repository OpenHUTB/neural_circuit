#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot -90 --myrot 0 --mzrot 0 --dsgc_thetax -4 --dsgc_thetay 8 --dsgc_thetaz 15 --n_dbp1 0 --n_ams 0 --ninfo 2 --disp_dsgc_zmin -15 --dsgc_nscale -3.02 -d 9 -v | vid -c -B 0 | ps2pdf - > x.pdf


# rotated thetaz 90
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot -90 --myrot 0 --mzrot 0 --dsgc_thetax -5 --dsgc_thetay 8 --dsgc_thetaz 90 --n_dbp1 0 --n_ams 0 --ninfo 2 --disp_dsgc_zmin -35 --disp_dsgc_zmax 0 --dsgc_nscale -3.02 -d 1 -v | vid

