retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot -90 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --disp_dsgc_zmin -35 --disp_dsgc_zmax 0 --dsgc_nscale -3.09 --mxrot 0 --dsgc_flip 1 --dsgc_denddia 0.8 -d 1 -v | vidpdf > dend_dive_90.pdf

retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --disp_dsgc_zmin -35 --disp_dsgc_zmax 0 --dsgc_nscale -3.01 --mxrot 0 --dsgc_flip 1 --dsgc_denddia 0.8 -d 9 -v | vidpdf > dend_dive_nodes.pdf

