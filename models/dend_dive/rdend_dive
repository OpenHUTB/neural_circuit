#  make movie
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 0.5 --make_movie 1 --space_time 0 --Vmax -0.057 --Vmin -0.065 --colormap 1 --backgnd 0 -v | vid -B 0

retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 0.8 --make_movie 1 --space_time 0 --Vmax -0.04 --Vmin -0.085 --colormap 3 --backgnd 0 -d 32 --frame_int 0.002 -v | vid


# make .ps files for movie
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 180 --dsgc_thetax -5 --dsgc_thetay 6 --dsgc_thetaz 90 --n_dbp1 0 --n_ams 0 --ninfo 2 --dsgc_nscale -3.09 --mxrot 0 --istim 50e-12 --sinc 0.05 --dsgc_flip 1 --dsgc_denddia 0.5 --make_movie 1 --space_time 1 --Vmax -0.04 --Vmin -0.073 --colormap 1 --backgnd 0 --frame_int 0.002 -v | vid -B 0 -c -P dend_dive_


