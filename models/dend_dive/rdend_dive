
# dsgc morphology with crossovers from on - off layers
#
retsim --expt dsgc_cbp_bar --dsgc_file morph_ww_sample_cell_crossover --mxrot -90 --n_dbp1 0 --n_ams 0 --ninfo 2 -d 1 -v | vid

# with densfile
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot -90 --myrot -30 --zrot -20 --n_dbp1 0 --n_ams 0 --ninfo 2 -d 1 -v | vid

# with thetax,y
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot -90 --myrot 0 --mzrot 0 --dsgc_thetax -5 --dsgc_thetay 8 --dsgc_thetaz 0 --n_dbp1 0 --n_ams 0 --ninfo 2 -d 1 -v | vid
makestim 0

#  with thetax,y,z
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot -90 --myrot 0 --mzrot 0 --dsgc_thetax -4 --dsgc_thetay 8 --dsgc_thetaz 15 --n_dbp1 0 --n_ams 0 --ninfo 2 -d 1 -v | vid

#  display nodes
#
retsim --expt dsgc_dend_dive --dsgc_file morph_ww_sample_cell_crossover --dsgc_densfile dens_dsgc_morph_ww.n --mxrot 0 --myrot 0 --mzrot 0 --dsgc_thetax -4 --dsgc_thetay 8 --dsgc_thetaz 15 --n_dbp1 0 --n_ams 0 --ninfo 2 --disp_dsgc_zmin -28 --dsgc_nscale -3.02 -d 9 -v | vid -c -B 0 | ps2pdf - > x.pdf 

# with somz -32 sen in expt_dsgc_dend_dive.cc
